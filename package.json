{
  "name": "extension-typescript-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate project to quickly build a manifest v2 or v3 web extension using TypeScript and WebPack.",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack -w --config webpack.dev.js"
  },
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "8.1.1",
    "@types/chrome": "0.0.139",
    "ts-loader": "9.1.2",
    "typescript": "4.2.4",
    "webpack": "5.37.0",
    "webpack-cli": "4.7.0",
    "webpack-merge": "5.7.3"
  }
}
